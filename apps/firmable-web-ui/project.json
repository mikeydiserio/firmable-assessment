{
	"name": "firmable-web-ui",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "firmable-web-ui/src",
	"projectType": "application",
	"tags": [],
	"targets": {
		"dev": {
			"executor": "@nx/next:dev",
			"options": {
				"outputPath": "dist/apps/firmable-web-ui"
			}
		},
		"build": {
			"executor": "@nx/next:build",
			"outputs": [
				"{workspaceRoot}/dist/apps/{projectRoot}"
			],
			"options": {
				"outputPath": "dist/apps/firmable-web-ui",
				"buildDepsTarget": "firmable-web-ui:build-deps",
				"watchDepsTarget": "firmable-web-ui:watch-deps"
			}
		},
		"build-deps": {
			"executor": "@nx/next:build-deps",
			"options": {
				"outputPath": "dist/apps/firmable-web-ui"
			}
		},
		"test": {
			"globals": true,
			"cache": true,
			"environment": "jsdom",
			"deps": {
				"inline": [
					"vitest-canvas-mock"
				]
			},
			"include": [
				"src/**/*.{test,spec}.{js,mjs,cjs,ts,mts,cts,jsx,tsx}"
			],
			"setupFiles": "./test-setup.ts",
			"environmentOptions": {
				"jsdom": {
					"resources": "usable"
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"options": {
				"lintFilePatterns": [
					"apps/firmable-web-ui/**/*.{ts,tsx,js,jsx}"
				]
			}
		}
	}
}