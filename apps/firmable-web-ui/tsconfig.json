{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/data/*": [
        "./src/data/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ],
      "@/public/*": [
        "./public/*"
      ],
      "@/types/*": [
        "./src/types/*"
      ],
      "@/mocks/*": [
        "./src/mocks/*"
      ],
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ]
    },
    "types": [
      "node",
      "jest"
    ],
    "target": "es2020"
  },
  "include": [
    ".next/types/**/*.ts"
  ],
  "files": [],
}