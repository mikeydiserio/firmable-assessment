{
  "name": "firmable-workspace",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.12.2",
    "pnpm": ">=8.10.4"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "clean": "npx rimraf {,**/*/}{node_modules,dist,.next} && lerna clean --y",
    "dev": "nx run-many -t next dev --turbopack",
    "monorepo:install": "pnpm install",
    "reset": "lerna clean --y && pnpm store prune && nx reset && pnpm monorepo:install",
    "start:graph": "nx graph",
    "build": "nx build /apps",
    "test": "nx run-many -t test",
    "lint": "nx run-many -t lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@eslint/compat": "^1.3.1",
    "@next/bundle-analyzer": "^15.3.5",
    "@next/eslint-plugin-next": "^15.3.5",
    "@nx/react": "^21.2.3",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "eslint-plugin-eslint-plugin": "^6.5.0",
    "eslint-plugin-jsdoc": "^51.4.0",
    "jest-environment-jsdom": "^30.0.4",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "normalize.css": "^8.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-is": "19.0.0",
    "react-router-dom": "6.11.2",
    "react-test-renderer": "19.1.0",
    "semver": "^7.7.2",
    "styled-components": "^6.1.19",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.36.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@nrwl/nx-cloud": "^19.1.0",
    "@nx/devkit": "21.2.3",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "^21.2.3",
    "@nx/jest": "^21.2.3",
    "@nx/js": "^21.2.3",
    "@nx/next": "21.2.3",
    "@nx/playwright": "21.2.3",
    "@nx/react": "^21.2.3",
    "@nx/storybook": "^21.2.3",
    "@nx/vite": "21.2.3",
    "@nx/web": "21.2.3",
    "@nx/workspace": "21.2.3",
    "@playwright/test": "^1.54.1",
    "@swc-node/register": "^1.10.10",
    "@swc/cli": "~0.6.0",
    "@swc/core": "^1.12.14",
    "@swc/helpers": "~0.5.11",
    "@swc/plugin-styled-components": "^1.5.67",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/debug": "^4.1.12",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/jest": "^30.0.0",
    "@types/natural-compare": "^1.4.3",
    "@types/node": "^20.19.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-is": "19.0.0",
    "@types/semver": "^7.5.8",
    "@types/styled-components": "5.1.26",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vitest/ui": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "babel-plugin-styled-components": "1.10.7",
    "console-fail-test": "^0.5.0",
    "cross-fetch": "^4.0.0",
    "cspell": "^9.0.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.0.16",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lerna": "8.2.3",
    "npm-run-all": "^4.1.5",
    "nx": "21.2.3",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.5",
    "semver": "7.7.0",
    "supabase": "^2.30.4",
    "ts-jest": "^29.1.0",
    "tsx": "*",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.0",
    "vitest-canvas-mock": "^0.3.3"
  },
  "nx": {
    "name": "repo",
    "includedScripts": [
      "generate-configs",
      "generate-lib",
      "generate-sponsors"
    ],
    "targets": {
      "generate-configs": {
        "dependsOn": [
          "eslint-plugin:build"
        ]
      },
      "generate-lib": {
        "dependsOn": [
          "typescript-eslint:build",
          "eslint-plugin-internal:build"
        ]
      },
      "// These targets are used for repo level utils and checking repo files which do not belong to specific published packages": {},
      "typecheck": {
        "command": "tsc -b tsconfig.repo-config-files.json",
        "dependsOn": [
          "types:copy-ast-spec"
        ],
        "outputs": [
          "{workspaceRoot}/dist"
        ],
        "cache": true
      },
      "lint": {
        "command": "eslint",
        "options": {
          "cache": true,
          "ignore-pattern": "packages"
        },
        "cache": false
      },
      "clean": {
        "command": "rimraf dist/ coverage/ .eslintcache"
      },
      "lint-staged": {
        "*.{ts,js,json,md}": [
          "prettier --write"
        ]
      }
    }
  }
}
